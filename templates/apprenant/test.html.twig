{% extends 'base.html.twig' %}




{% block body %}

  <body>
    <div id="root">


      <main>
        <div class="container">
          <!-- Title and Top Buttons Start -->
          <div class="page-title-container">
            <div class="row g-0">
              <div class="col-auto mb-2 mb-md-0 me-auto">
                <div class="w-auto sw-md-30">
                  <h1 class="mb-0 pb-0 display-4" id="title">Course List</h1>
                  <nav class="breadcrumb-container d-inline-block" aria-label="breadcrumb">
                    <ul class="breadcrumb pt-0">
                      <li class="breadcrumb-item"><a href="{{path('app_front')}}">Home</a></li>
                      <li class="breadcrumb-item"><a href="{{path('app_cours_show')}}">Courses</a></li>
                    </ul>
                  </nav>
                </div>
              </div>
              <div class="col-auto d-flex d-lg-none align-items-start mb-2 mb-md-0 order-md-1">
                <button type="button" class="btn btn-primary btn-icon btn-icon-only ms-1" id="menuButton">
                  <i data-acorn-icon="menu-left"></i>
                </button>
              </div>
             
            </div>
          </div>
          <!-- Title and Top Buttons End -->

          <!-- Content Start -->
          <div class="row g-0">
            <div class="col-auto d-none d-lg-flex">
              <div class="nav flex-column sw-30 pe-7" id="menuColumn">
                <!-- Content of this will be moved from #menuMoveContent div based on the responsive breakpoint.  -->
              </div>
            </div>

            <div class="col">
              <!-- List Items Start -->
              <div class="row g-3 row-cols-1 row-cols-md-2 row-cols-xl-3 row-cols-xxl-4 mb-5">
              {% for cour in cours %}
                <div class="col">
                  <div class="card h-100">
                    <img id="image" src="{{ asset('img/course/small/'~ cour.image) }}"  
                            class="card-img-top sh-22" alt="card image"> 
                    <div class="card-body">
                      <h5 class="heading mb-0"><a href="Course.Detail.html" class="body-link stretched-link">{{cour.titreCours}}</a></h5>
                    </div>
                    <div class="card-footer border-0 pt-0">
                     
                      <div class="card-text mb-0">
                        <div class="text">
                          <div>{{cour.idCategorie}}</div>
                        </div>
                        <div>$ {{cour.prix}}</div>
                      </div>
                    </div>
                  </div>
                </div>
                {% endfor %}
                
               
        
              <!-- List Items End -->
            </div>
          </div>
      
          <!-- Menu Content Start -->
          <div class="modal modal-right fade" id="menuModal" tabindex="-1" role="dialog" aria-hidden="true">
            <div class="modal-dialog">
              <div class="modal-content">
                <div class="modal-header">
                  <h5 class="modal-title">Course Filters</h5>
                  <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                </div>
              
                
                 
                <div class="modal-body">
                
                  <!-- Content of below will be moved to #menuColumn or back in here based on the data-move-breakpoint attribute below -->
                  <!-- Content will be here if the screen size is smaller than lg -->
                  <div id="menuMoveContent" data-move-target="#menuColumn" data-move-breakpoint="lg">
                    <div class="menu-items">
                    {{ form_start(form) }}
                      <div class="mb-5">
                        <p class="text-small text-muted mb-2">SEARCH</p>
                        <div class="mt-3">
                        <div class="me-lg-auto w-100 w-md-auto search-input-container border border-separator">
        
                        {{ form_row(form.mot,{'attr':{'class':'form-control'}}) }}
                        </div>
                        </div>
                      </div>

                       <div class="mb-5">
                        <p class="text-small text-muted mb-2">CATEGORY</p>
                        
        
                        {{ form_row(form.categorie,{'attr':{'class':'form-control'}}) }}
                        
                      </div>
                     
         
  
                
        
            

                      <div class="mb-5">
                        <p class="text-small text-muted mb-2">PRICE</p>
                        <div class="row g-1">
                          <div class="col">
                            {{ form_row(form.min,{'attr':{'class':'form-control'}}) }}
                           
                          </div>
                          <div class="col">
                            {{ form_row(form.max,{'attr':{'class':'form-control'}}) }}
                          </div>
                          
                        </div>
                      </div>


                   

                    
                      <div class="mb-5">
                        <div class="d-flex flex-row justify-content-between w-100">
                          
                        
                          {{ form_row(form.enregistrer,{'attr':{'class':'btn btn-primary w-100'}}) }}
                        
                          
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
                {{ form_end(form) }}
              </div>
            </div>
          
          </div>
          <!-- Menu Content End -->

          <!-- Content End -->
        </div>
